   sprites.js â€“ Procedural sprite atlas generator
   Inspired by C7-Game terrain layers, Unciv tileset
   system, and Freeciv-web's sprite sheet approach.
   All art drawn procedurally on offscreen canvases.
   â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ */

const SPRITE_SIZE = 64;

// â”€â”€â”€â”€â”€ Utility â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function offscreen(w = SPRITE_SIZE, h = SPRITE_SIZE) {
  const c = document.createElement('canvas');
  c.width = w; c.height = h;
  return c;
}

function seededRandom(seed) {
  let s = seed;
  return () => { s = (s * 16807 + 0) % 2147483647; return (s - 1) / 2147483646; };
}

// â”€â”€â”€â”€â”€ Terrain sprites â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function drawPlainsTile(seed = 42) {
  const c = offscreen();
  const ctx = c.getContext('2d');
  const rng = seededRandom(seed);

  // Base gradient
  const g = ctx.createLinearGradient(0, 0, 0, SPRITE_SIZE);
  g.addColorStop(0, '#5a9e5f');
  g.addColorStop(0.5, '#4d8f52');
  g.addColorStop(1, '#3f7d43');
  ctx.fillStyle = g;
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);

  // Grass blades
  ctx.strokeStyle = 'rgba(80,160,90,0.35)';
  ctx.lineWidth = 1;
  for (let i = 0; i < 18; i++) {
    const x = rng() * SPRITE_SIZE;
    const y = rng() * SPRITE_SIZE;
    ctx.beginPath();
    ctx.moveTo(x, y);
    ctx.lineTo(x + (rng() - 0.5) * 6, y - 4 - rng() * 6);
    ctx.stroke();
  }

  // Small flowers
  const flowerColors = ['#f7e26b', '#e8d44d', '#fff'];
  for (let i = 0; i < 3; i++) {
    const x = rng() * SPRITE_SIZE;
    const y = rng() * SPRITE_SIZE;
    ctx.fillStyle = flowerColors[Math.floor(rng() * flowerColors.length)];
    ctx.beginPath();
    ctx.arc(x, y, 1.2, 0, Math.PI * 2);
    ctx.fill();
  }

  return c;
}

function drawForestTile(seed = 77) {
  const c = offscreen();
  const ctx = c.getContext('2d');
  const rng = seededRandom(seed);

  // Dark base
  const g = ctx.createLinearGradient(0, 0, 0, SPRITE_SIZE);
  g.addColorStop(0, '#2e6e3a');
  g.addColorStop(1, '#1a4d26');
  ctx.fillStyle = g;
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);

  // Trees (triangular conifers + round deciduous)
  const treeCount = 4 + Math.floor(rng() * 3);
  for (let i = 0; i < treeCount; i++) {
    const tx = 8 + rng() * (SPRITE_SIZE - 16);
    const ty = 12 + rng() * (SPRITE_SIZE - 20);
    const size = 8 + rng() * 6;
    const type = rng() > 0.5 ? 'conifer' : 'deciduous';

    // Shadow
    ctx.fillStyle = 'rgba(0,0,0,0.15)';
    ctx.beginPath();
    ctx.ellipse(tx + 2, ty + 3, size * 0.5, size * 0.25, 0, 0, Math.PI * 2);
    ctx.fill();

    if (type === 'conifer') {
      // Trunk
      ctx.fillStyle = '#5a3a1e';
      ctx.fillRect(tx - 1.5, ty - 2, 3, 6);
      // Foliage triangle
      ctx.fillStyle = `rgb(${30 + Math.floor(rng() * 30)},${90 + Math.floor(rng() * 40)},${30 + Math.floor(rng() * 20)})`;
      ctx.beginPath();
      ctx.moveTo(tx, ty - size);
      ctx.lineTo(tx - size * 0.5, ty);
      ctx.lineTo(tx + size * 0.5, ty);
      ctx.closePath();
      ctx.fill();
    } else {
      // Trunk
      ctx.fillStyle = '#6b4423';
      ctx.fillRect(tx - 1.5, ty - 2, 3, 7);
      // Round canopy
      ctx.fillStyle = `rgb(${40 + Math.floor(rng() * 30)},${100 + Math.floor(rng() * 50)},${30 + Math.floor(rng() * 20)})`;
      ctx.beginPath();
      ctx.arc(tx, ty - size * 0.45, size * 0.5, 0, Math.PI * 2);
      ctx.fill();
      // Highlight
      ctx.fillStyle = 'rgba(120,200,100,0.2)';
      ctx.beginPath();
      ctx.arc(tx - 1, ty - size * 0.55, size * 0.25, 0, Math.PI * 2);
      ctx.fill();
    }
  }

  return c;
}

function drawHillTile(seed = 33) {
  const c = offscreen();
  const ctx = c.getContext('2d');
  const rng = seededRandom(seed);

  // Base
  const g = ctx.createLinearGradient(0, 0, 0, SPRITE_SIZE);
  g.addColorStop(0, '#8a7d50');
  g.addColorStop(0.5, '#7a6d42');
  g.addColorStop(1, '#6a5d35');
  ctx.fillStyle = g;
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);

  // Elevated hills (2-3 mounds)
  const hillCount = 2 + Math.floor(rng() * 2);
  for (let i = 0; i < hillCount; i++) {
    const hx = 10 + rng() * (SPRITE_SIZE - 20);
    const hy = 20 + rng() * (SPRITE_SIZE - 30);
    const hw = 18 + rng() * 14;
    const hh = 10 + rng() * 8;

    // Hill shadow
    ctx.fillStyle = 'rgba(0,0,0,0.12)';
    ctx.beginPath();
    ctx.ellipse(hx + 3, hy + 4, hw * 0.55, hh * 0.5, 0, 0, Math.PI * 2);
    ctx.fill();

    // Hill body
    const hg = ctx.createRadialGradient(hx - 3, hy - 3, 0, hx, hy, hw * 0.6);
    hg.addColorStop(0, '#b5a872');
    hg.addColorStop(1, '#8a7a4e');
    ctx.fillStyle = hg;
    ctx.beginPath();
    ctx.ellipse(hx, hy, hw * 0.5, hh * 0.5, 0, Math.PI, 0);
    ctx.fill();

    // Rocky texture
    ctx.fillStyle = 'rgba(100,90,60,0.3)';
    for (let j = 0; j < 4; j++) {
      const rx = hx - hw * 0.3 + rng() * hw * 0.6;
      const ry = hy - hh * 0.3 + rng() * hh * 0.3;
      ctx.fillRect(rx, ry, 2 + rng() * 3, 1 + rng() * 2);
    }
  }

  // Sparse grass
  ctx.strokeStyle = 'rgba(100,140,60,0.25)';
  ctx.lineWidth = 0.8;
  for (let i = 0; i < 6; i++) {
    const x = rng() * SPRITE_SIZE;
    const y = SPRITE_SIZE * 0.6 + rng() * SPRITE_SIZE * 0.35;
    ctx.beginPath();
    ctx.moveTo(x, y);
    ctx.lineTo(x + (rng() - 0.5) * 4, y - 3 - rng() * 4);
    ctx.stroke();
  }

  return c;
}

function drawWaterTile(seed = 55) {
  const c = offscreen();
  const ctx = c.getContext('2d');
  const rng = seededRandom(seed);

  // Deep gradient
  const g = ctx.createLinearGradient(0, 0, SPRITE_SIZE, SPRITE_SIZE);
  g.addColorStop(0, '#1e5799');
  g.addColorStop(0.5, '#2469aa');
  g.addColorStop(1, '#1a4f88');
  ctx.fillStyle = g;
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);

  // Wave highlights
  ctx.strokeStyle = 'rgba(120,180,240,0.25)';
  ctx.lineWidth = 1;
  for (let i = 0; i < 6; i++) {
    const y = 6 + rng() * (SPRITE_SIZE - 12);
    const startX = rng() * 10;
    ctx.beginPath();
    ctx.moveTo(startX, y);
    for (let wx = startX; wx < SPRITE_SIZE; wx += 8) {
      ctx.quadraticCurveTo(wx + 4, y - 2 - rng() * 2, wx + 8, y);
    }
    ctx.stroke();
  }

  // Specular highlights
  ctx.fillStyle = 'rgba(180,220,255,0.08)';
  for (let i = 0; i < 4; i++) {
    const x = rng() * SPRITE_SIZE;
    const y = rng() * SPRITE_SIZE;
    ctx.beginPath();
    ctx.ellipse(x, y, 3 + rng() * 4, 1 + rng() * 2, rng() * Math.PI, 0, Math.PI * 2);
    ctx.fill();
  }

  return c;
}

function drawDesertTile(seed = 88) {
  const c = offscreen();
  const ctx = c.getContext('2d');
  const rng = seededRandom(seed);

  const g = ctx.createLinearGradient(0, 0, 0, SPRITE_SIZE);
  g.addColorStop(0, '#d4b96a');
  g.addColorStop(0.5, '#c9a855');
  g.addColorStop(1, '#b8973e');
  ctx.fillStyle = g;
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);

  // Sand dunes
  ctx.strokeStyle = 'rgba(180,150,80,0.3)';
  ctx.lineWidth = 1.5;
  for (let i = 0; i < 4; i++) {
    const y = 10 + rng() * (SPRITE_SIZE - 20);
    ctx.beginPath();
    ctx.moveTo(0, y);
    for (let x = 0; x < SPRITE_SIZE; x += 12) {
      ctx.quadraticCurveTo(x + 6, y - 3 + rng() * 6, x + 12, y);
    }
    ctx.stroke();
  }

  // Sand grains
  ctx.fillStyle = 'rgba(200,180,120,0.2)';
  for (let i = 0; i < 20; i++) {
    ctx.fillRect(rng() * SPRITE_SIZE, rng() * SPRITE_SIZE, 1, 1);
  }

  return c;
}

// â”€â”€â”€â”€â”€ Unit sprites â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function drawWarriorSprite(color, isBot = false) {
  const c = offscreen(48, 48);
  const ctx = c.getContext('2d');
  const cx = 24, cy = 24;

  // Shield glow
  ctx.shadowColor = color;
  ctx.shadowBlur = 8;
  ctx.fillStyle = color;
  ctx.beginPath();
  ctx.arc(cx, cy, 16, 0, Math.PI * 2);
  ctx.fill();
  ctx.shadowBlur = 0;

  // Body
  ctx.fillStyle = isBot ? '#cc3333' : '#3388cc';
  ctx.beginPath();
  ctx.ellipse(cx, cy + 2, 10, 12, 0, 0, Math.PI * 2);
  ctx.fill();

  // Armor plates
  const armorColor = isBot ? '#aa2222' : '#2266aa';
  ctx.fillStyle = armorColor;
  ctx.beginPath();
  ctx.ellipse(cx, cy, 8, 9, 0, 0, Math.PI * 2);
  ctx.fill();

  // Head
  ctx.fillStyle = '#ffd4a3';
  ctx.beginPath();
  ctx.arc(cx, cy - 8, 5, 0, Math.PI * 2);
  ctx.fill();

  // Helmet
  ctx.fillStyle = isBot ? '#882222' : '#224488';
  ctx.beginPath();
  ctx.arc(cx, cy - 10, 5, Math.PI, 0);
  ctx.fill();

  // Eyes
  ctx.fillStyle = '#222';
  ctx.fillRect(cx - 3, cy - 9, 2, 1.5);
  ctx.fillRect(cx + 1, cy - 9, 2, 1.5);

  // Weapon
  if (isBot) {
    // Bot: axe
    ctx.strokeStyle = '#8b6914';
    ctx.lineWidth = 2;
    ctx.beginPath();
    ctx.moveTo(cx + 10, cy - 6);
    ctx.lineTo(cx + 10, cy + 10);
    ctx.stroke();
    ctx.fillStyle = '#999';
    ctx.beginPath();
    ctx.moveTo(cx + 7, cy - 6);
    ctx.lineTo(cx + 13, cy - 6);
    ctx.lineTo(cx + 14, cy - 2);
    ctx.lineTo(cx + 7, cy - 2);
    ctx.closePath();
    ctx.fill();
  } else {
    // Player: sword
    ctx.strokeStyle = '#c0c0c0';
    ctx.lineWidth = 2.5;
    ctx.beginPath();
    ctx.moveTo(cx + 10, cy - 10);
    ctx.lineTo(cx + 10, cy + 6);
    ctx.stroke();
    // Guard
    ctx.strokeStyle = '#8b6914';
    ctx.lineWidth = 2;
    ctx.beginPath();
    ctx.moveTo(cx + 6, cy + 1);
    ctx.lineTo(cx + 14, cy + 1);
    ctx.stroke();
  }

  // Shield (left hand)
  ctx.fillStyle = isBot ? '#661111' : '#114466';
  ctx.strokeStyle = isBot ? '#882222' : '#2266aa';
  ctx.lineWidth = 1.5;
  ctx.beginPath();
  ctx.ellipse(cx - 9, cy + 1, 5, 7, -0.2, 0, Math.PI * 2);
  ctx.fill();
  ctx.stroke();

  // Shield emblem
  ctx.fillStyle = isBot ? '#ff4444' : '#44aaff';
  ctx.beginPath();
  ctx.arc(cx - 9, cy, 2.5, 0, Math.PI * 2);
  ctx.fill();

  return c;
}

// â”€â”€â”€â”€â”€ City sprites â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function drawCitySprite(isPlayer, size = 'small') {
  const w = 56, h = 56;
  const c = offscreen(w, h);
  const ctx = c.getContext('2d');
  const cx = w / 2, cy = h / 2;

  const primary = isPlayer ? '#4488cc' : '#cc4444';
  const secondary = isPlayer ? '#336699' : '#993333';
  const wall = isPlayer ? '#5599dd' : '#dd5555';
  const roof = isPlayer ? '#2255aa' : '#aa2233';

  // City glow
  ctx.shadowColor = primary;
  ctx.shadowBlur = 10;
  ctx.fillStyle = primary + '40';
  ctx.beginPath();
  ctx.arc(cx, cy, 24, 0, Math.PI * 2);
  ctx.fill();
  ctx.shadowBlur = 0;

  // Walls
  ctx.fillStyle = wall;
  ctx.strokeStyle = secondary;
  ctx.lineWidth = 1.5;
  const wallY = cy + 4;
  ctx.beginPath();
  ctx.roundRect(cx - 20, wallY, 40, 14, 3);
  ctx.fill();
  ctx.stroke();

  // Wall battlements
  ctx.fillStyle = secondary;
  for (let i = 0; i < 5; i++) {
    ctx.fillRect(cx - 18 + i * 9, wallY - 3, 5, 4);
  }

  // Main building
  ctx.fillStyle = wall;
  ctx.fillRect(cx - 8, cy - 6, 16, 16);
  ctx.fillStyle = roof;
  // Roof
  ctx.beginPath();
  ctx.moveTo(cx - 10, cy - 6);
  ctx.lineTo(cx, cy - 16);
  ctx.lineTo(cx + 10, cy - 6);
  ctx.closePath();
  ctx.fill();

  // Tower left
  ctx.fillStyle = wall;
  ctx.fillRect(cx - 16, cy - 2, 8, 12);
  ctx.fillStyle = roof;
  ctx.beginPath();
  ctx.moveTo(cx - 18, cy - 2);
  ctx.lineTo(cx - 12, cy - 10);
  ctx.lineTo(cx - 6, cy - 2);
  ctx.closePath();
  ctx.fill();

  // Tower right
  ctx.fillStyle = wall;
  ctx.fillRect(cx + 8, cy - 2, 8, 12);
  ctx.fillStyle = roof;
  ctx.beginPath();
  ctx.moveTo(cx + 6, cy - 2);
  ctx.lineTo(cx + 12, cy - 10);
  ctx.lineTo(cx + 18, cy - 2);
  ctx.closePath();
  ctx.fill();

  // Windows
  ctx.fillStyle = '#ffeebb';
  ctx.fillRect(cx - 3, cy - 2, 3, 3);
  ctx.fillRect(cx + 1, cy - 2, 3, 3);
  ctx.fillRect(cx - 3, cy + 3, 3, 3);
  ctx.fillRect(cx + 1, cy + 3, 3, 3);

  // Door
  ctx.fillStyle = '#6b4423';
  ctx.beginPath();
  ctx.arc(cx, cy + 10, 3, Math.PI, 0);
  ctx.fill();
  ctx.fillRect(cx - 3, cy + 10, 6, 4);

  // Flag
  ctx.strokeStyle = '#666';
  ctx.lineWidth = 1.5;
  ctx.beginPath();
  ctx.moveTo(cx, cy - 16);
  ctx.lineTo(cx, cy - 24);
  ctx.stroke();
  ctx.fillStyle = isPlayer ? '#44aaff' : '#ff4444';
  ctx.beginPath();
  ctx.moveTo(cx, cy - 24);
  ctx.lineTo(cx + 7, cy - 21);
  ctx.lineTo(cx, cy - 18);
  ctx.closePath();
  ctx.fill();

  return c;
}

// â”€â”€â”€â”€â”€ Effect sprites â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function drawExplosionFrame(frame, maxFrames = 6) {
  const c = offscreen(48, 48);
  const ctx = c.getContext('2d');
  const cx = 24, cy = 24;
  const progress = frame / maxFrames;
  const radius = 6 + progress * 18;
  const alpha = 1 - progress * 0.8;

  // Outer glow
  const g = ctx.createRadialGradient(cx, cy, 0, cx, cy, radius);
  g.addColorStop(0, `rgba(255,200,50,${alpha})`);
  g.addColorStop(0.4, `rgba(255,100,20,${alpha * 0.7})`);
  g.addColorStop(1, `rgba(180,30,0,0)`);
  ctx.fillStyle = g;
  ctx.beginPath();
  ctx.arc(cx, cy, radius, 0, Math.PI * 2);
  ctx.fill();

  // Sparks
  const rng = seededRandom(frame * 31);
  ctx.fillStyle = `rgba(255,230,100,${alpha})`;
  for (let i = 0; i < 6; i++) {
    const angle = rng() * Math.PI * 2;
    const dist = radius * (0.5 + rng() * 0.7);
    ctx.beginPath();
    ctx.arc(cx + Math.cos(angle) * dist, cy + Math.sin(angle) * dist, 1.5 - progress, 0, Math.PI * 2);
    ctx.fill();
  }

  return c;
}

function drawSelectionRing() {
  const c = offscreen(64, 64);
  const ctx = c.getContext('2d');
  ctx.strokeStyle = 'rgba(90,200,250,0.85)';
  ctx.lineWidth = 2.5;
  ctx.setLineDash([6, 4]);
  ctx.beginPath();
  ctx.arc(32, 32, 22, 0, Math.PI * 2);
  ctx.stroke();
  return c;
}

function drawMoveTarget() {
  const c = offscreen(24, 24);
  const ctx = c.getContext('2d');
  ctx.strokeStyle = 'rgba(90,250,150,0.7)';
  ctx.lineWidth = 2;
  ctx.beginPath();
  ctx.arc(12, 12, 8, 0, Math.PI * 2);
  ctx.stroke();
  ctx.beginPath();
  ctx.arc(12, 12, 3, 0, Math.PI * 2);
  ctx.fill();
  return c;
}

// â”€â”€â”€â”€â”€ Fog of war texture â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function drawFogTexture() {
  const c = offscreen(SPRITE_SIZE, SPRITE_SIZE);
  const ctx = c.getContext('2d');
  const rng = seededRandom(123);

  ctx.fillStyle = 'rgba(8,12,20,0.7)';
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);

  // Cloudy fog effect
  for (let i = 0; i < 30; i++) {
    const x = rng() * SPRITE_SIZE;
    const y = rng() * SPRITE_SIZE;
    const r = 6 + rng() * 14;
    const g = ctx.createRadialGradient(x, y, 0, x, y, r);
    g.addColorStop(0, `rgba(15,20,35,${0.1 + rng() * 0.15})`);
    g.addColorStop(1, 'rgba(15,20,35,0)');
    ctx.fillStyle = g;
    ctx.beginPath();
    ctx.arc(x, y, r, 0, Math.PI * 2);
    ctx.fill();
  }

  return c;
}

function drawSeenFogTexture() {
  const c = offscreen(SPRITE_SIZE, SPRITE_SIZE);
  const ctx = c.getContext('2d');
  ctx.fillStyle = 'rgba(8,12,20,0.45)';
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);
  return c;
}

function drawUnknownTexture() {
  const c = offscreen(SPRITE_SIZE, SPRITE_SIZE);
  const ctx = c.getContext('2d');
  ctx.fillStyle = '#070b14';
  ctx.fillRect(0, 0, SPRITE_SIZE, SPRITE_SIZE);
  return c;
}

